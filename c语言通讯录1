contact.h 文件名源文件
#define _CRT_SECURE_NO_WARNINGS 1
#define MAX 1000
#define MAX_NAME 20
#define MAX_SEX 5
#define MAX_TELE 12
#define MAX_ADDR 20

#include <stdio.h>

struct peoInfo
{
char name[MAX_NAME];
int age;
char sex[MAX_SEX];
char tele[MAX_TELE];
char addr[MAX_ADDR];
};
struct Contact
{
	struct peoinfo data[MAX];
	int size;
};

//声明函数
void InitContact(struct Contact* ps);
AddContact(struct Contact* ps);
AddContact(const struct Contact* ps);
